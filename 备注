pip install paddlepaddle  # PaddlePaddle深度学习框架
pip install "paddleocr>=2.6.1.3"  # PaddleOCR库

# 检查PaddlePaddle是否正常
    import paddle

    print(paddle.__version__)  # 应输出类似 2.6.1

    # 检查PaddleOCR是否正常
    from paddleocr import PaddleOCR

    print(PaddleOCR.__version__)  # 应输出类似 2.7.0.3

    pip list | findstr paddlepaddle
